{
  "name": "@ideal-postcodes/postcode-lookup-bundled",
  "version": "1.0.0",
  "description": "JS bundles for Postcode Lookup library",
  "author": {
    "name": "Ideal Postcodes",
    "email": "support@ideal-postcodes.co.uk",
    "url": "https://ideal-postcodes.co.uk"
  },
  "bugs": {
    "url": "https://github.com/ideal-postcodes/postcode-lookup-bundled/issues",
    "email": "support@ideal-postcodes.co.uk"
  },
  "homepage": "https://github.com/ideal-postcodes/postcode-lookup-bundled",
  "repository": {
    "type": "git",
    "url": "https://github.com/ideal-postcodes/postcode-lookup-bundled.git"
  },
  "scripts": {
    "semantic-release": "semantic-release --no-ci",
    "start": "http-server",
    "build": "rollup -c",
    "test": "npm run test:local:umd && npm run test:local:esm",
    "test:local:umd": "karma start .config/local.umd.ts",
    "test:local:esm": "karma start .config/local.esm.ts",
    "test:ie": "karma start .config/ie11.ts",
    "test:umd": "karma start .config/umd.ts",
    "test:esm": "karma start .config/esm.ts",
    "test:modern": "karma start .config/modern.ts",
    "prepublishOnly": "npm run build"
  },
  "main": "dist/postcode-lookup.umd.min.js",
  "module": "dist/postcode-lookup.esm.modern.min.js",
  "files": [
    "dist"
  ],
  "prettier": {
    "trailingComma": "es5"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "API",
    "UK",
    "Address Search",
    "Postcode Lookup",
    "Search",
    "Ideal Postcodes"
  ],
  "release": {
    "extends": "@cablanchard/semantic-release/dist/npm",
    "branches": [
      "main",
      "next"
    ]
  },
  "license": "AGPL-3.0",
  "devDependencies": {
    "@babel/core": "~7.11.0",
    "@babel/preset-env": "~7.12.0",
    "@cablanchard/semantic-release": "~1.3.0",
    "@cablanchard/tsconfig": "~1.0.0",
    "@ideal-postcodes/postcode-lookup": "~1.0.1",
    "@ideal-postcodes/supported-browsers": "~2.2.0",
    "@open-wc/karma-esm": "~3.0.0",
    "@rollup/plugin-commonjs": "~15.1.0",
    "@rollup/plugin-node-resolve": "~9.0.0",
    "@types/mocha": "~8.0.0",
    "@types/puppeteer": "~3.0.0",
    "core-js": "~3.6.0",
    "dotenv": "~8.2.0",
    "http-server": "~0.12.0",
    "karma": "~5.2.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-mocha": "~2.0.0",
    "karma-sauce-launcher": "~1.2.0",
    "karma-typescript": "~5.2.0",
    "mocha": "~8.1.0",
    "prettier": "~2.1.0",
    "puppeteer": "~5.3.1",
    "rollup": "~2.30.0",
    "rollup-plugin-babel": "~4.4.0",
    "rollup-plugin-terser": "~7.0.0",
    "semantic-release": "~17.2.1",
    "ts-node": "~9.0.0",
    "typescript": "~4.0.3"
  },
  "dependencies": {}
}
